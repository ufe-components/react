@import './variables.styl'

clearfix()
  zoom 1
  &:before,
  &:after
    content ""
    display table
  &:after
    clear both

reset-component()
  font-family $font-family
  font-size $font-size-base
  line-height $line-height-base
  color $text-color
  box-sizing border-box
  margin 0
  padding 0
  list-style none
