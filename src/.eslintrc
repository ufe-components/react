{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "extends": ["standard", "standard-react"],
  "rules": {
    "no-self-compare": 0,
    "react/prop-types": [2, {
      "ignore": ["children", "className", "style"]
    }]
  }
}
